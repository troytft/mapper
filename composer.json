{
    "name": "troytft/mapper",
    "description": "Map data to models with annotations and transformers",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Mapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "~8.1.2"
    },
    "require": {
        "php": "^7.2",
        "doctrine/annotations": " ~1.6"
    }
}
