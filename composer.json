{
    "name": "troytft/mapper",
    "description": "Mapper is a PHP library for mapping data to objects, type of property can be configured by annotation",
    "keywords": ["mapper"],
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Mapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.15",
        "phpunit/phpunit": "~8.1.2",
        "slevomat/coding-standard": "~5.0",
        "squizlabs/php_codesniffer": "~3.4.2"
    },
    "require": {
        "php": "^7.2",
        "doctrine/annotations": " ~1.6"
    }
}
