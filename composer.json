{
    "name": "troytft/mapper",
    "abandoned": true,
    "description": "Mapper is a PHP library for mapping data to objects, type of property can be configured by annotation",
    "keywords": ["mapper"],
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Mapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require": {
        "php": ">=7.4",
        "doctrine/annotations": " ^1.11",
        "symfony/polyfill-php81": "^1.22"
    }
}
